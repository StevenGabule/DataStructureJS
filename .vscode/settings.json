{
	"cSpell.words": [
		"AAAABBBCCDAABBB",
		"jsweet",
		"Kata",
		"nums",
		"ooxx",
		"rehtona",
		"salva",
		"sroirraw",
		"wollef"
	],
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "always",
		"source.organizeImports": "always"
	},
	"eslint.validate": [
		"javascript",
		"javascriptreact"
	],
	"prettier.requireConfig": false
}