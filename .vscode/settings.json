{
	"cSpell.words": [
		"jsweet",
		"nums",
		"salva"
	],
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "always",
		"source.organizeImports": "always"
	},
	"eslint.validate": [
		"javascript",
		"javascriptreact"
	],
	"prettier.requireConfig": false
}